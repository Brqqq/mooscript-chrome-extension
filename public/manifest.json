{
  "manifest_version": 2,
  "version": "1.3.0",
  "name": "Mooscript",
  "icons": {
    "16": "logo16.png",
    "64": "logo64.png",
    "128": "logo128.png"
  },
  "description": "A script for mobstar.cc",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "options_page": "index.html",
  "browser_action": {
    "default_title": "Options"
  },
  
  "permissions": [
    "storage",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "https://www.mobstar.cc/*"
  ]
}
{
  "manifest_version": 2,
  "version": "1.0.0",
  "icons": {
    "16": "logo16.png",
    "64": "logo64.png",
    "128": "logo128.png"
  },
  "name": "Mooscript",
  "description": "A script for mobstar.cc",
  "background": {
    "page": "src/html/background.html"
  },
  "options_page": "index.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}